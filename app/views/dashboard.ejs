<!-- views/dashboard.ejs -->
<!DOCTYPE html>
<html>
    <head>
        
        <!-- Title -->
        <title>Dashboard | RMM</title>
        
        <meta content="width=device-width, initial-scale=1" name="viewport"/>
        <meta charset="UTF-8">
        <meta name="description" content="RMM Dashboard" />
        <meta name="keywords" content="RMM Dashboard" />
        <meta name="author" content="Rajesh Kumar Chukka" />
        
        <!-- Styles -->
        <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600' rel='stylesheet' type='text/css'>
        <link href="public/css/pace-theme-flash.css" rel="stylesheet"/>
        <link href="public/css/uniform.default.min.css" rel="stylesheet"/>
        <link href="public/css/bootstrap.min.css" rel="stylesheet" type="text/css"/>
        <link href="public/css/font-awesome.min.css" rel="stylesheet" type="text/css"/>
        <link href="public/css/simple-line-icons.css" rel="stylesheet" type="text/css"/>	
        <link href="public/css/menu_cornerbox.css" rel="stylesheet" type="text/css"/>	
        <link href="public/css/waves.min.css" rel="stylesheet" type="text/css"/>	
        <link href="public/css/switchery.min.css" rel="stylesheet" type="text/css"/>
        <link href="public/css/3d-bold-navigation-style.css" rel="stylesheet" type="text/css"/>
        <link href="public/css/slidepushmenus-component.css" rel="stylesheet" type="text/css"/>	
        <link href="public/css/weather-icons.min.css" rel="stylesheet" type="text/css"/>	
        <link href="public/css/MetroJs.min.css" rel="stylesheet" type="text/css"/>	
        <link href="public/css/toastr.min.css" rel="stylesheet" type="text/css"/>	
        	
        <!-- Theme Styles -->
        <link href="public/css/modern.min.css" rel="stylesheet" type="text/css"/>
        <link href="public/css/green.css" class="theme-color" rel="stylesheet" type="text/css"/>
        <link href="public/css/custom.css" rel="stylesheet" type="text/css"/>
        
        <!--<script src="public/js/modernizr.js"></script>-->
        <script src="public/js/snap.svg-min.js"></script>
        
        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

    </head>
    <body class="page-header-fixed">
        <div class="overlay"></div>
        
        <main class="page-content content-wrap">
            <% include partials/top-nav %>
            <!-- Navbar -->
            <% include partials/left-nav %>
            <!-- Page Sidebar -->
            <div class="page-inner">
                
                <div class="page-title">
                    <h3>Dashboard</h3>
                    <div class="page-breadcrumb">
                        <ol class="breadcrumb">
                            <li class="active">Home</li>
                            
                        </ol>
                    </div>
                </div>
                <div id="main-wrapper">
                    <div class="row wrapper_container">
                        <div class="col-lg-4 col-md-6">
                            <div class="panel info-box panel-white">
                                <div class="panel-body">
                                    <div class="info-box-stats">
                                        <p class="counter"><%= user_count %></p>
                                        <span class="info-box-title">User activity this month</span>
                                    </div>
                                    <div class="info-box-icon">
                                        <i class="fa fa-group"></i>
                                    </div>
                                    <div class="info-box-progress">
                                        <div class="progress progress-xs progress-squared bs-n">
                                            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="<%= user_count %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= user_count %>%">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6">
                            <div class="panel info-box panel-white">
                                <div class="panel-body">
                                    <div class="info-box-stats">
                                        <p class="counter"><%= resource_count %></p>
                                        <span class="info-box-title">Total resources available</span>
                                    </div>
                                    <div class="info-box-icon">
                                        <i class="fa fa-list"></i>
                                    </div>
                                    <div class="info-box-progress">
                                        <div class="progress progress-xs progress-squared bs-n">
                                            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="<%= resource_count %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= resource_count %>%">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6">
                            <div class="panel info-box panel-white">
                                <div class="panel-body">
                                    <div class="info-box-stats">
                                        <p class="counter"><%= visit_count %></p>
                                        <span class="info-box-title">Maintenance visits conducted this month</span>
                                    </div>
                                    <div class="info-box-icon">
                                        <i class="fa fa-suitcase"></i>
                                    </div>
                                    <div class="info-box-progress">
                                        <div class="progress progress-xs progress-squared bs-n">
                                            <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="<%= visit_count %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= visit_count %>%">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6">
                            <div class="panel info-box panel-white">
                                <div class="panel-body">
                                    <div class="info-box-stats">
                                        <p class="counter"><%= agency_count %></p>
                                        <span class="info-box-title">Maintenance agencies under contract</span>
                                    </div>
                                    <div class="info-box-icon">
                                        <i class="fa fa-building"></i>
                                    </div>
                                    <div class="info-box-progress">
                                        <div class="progress progress-xs progress-squared bs-n">
                                            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="<%= agency_count %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= agency_count %>%">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6">
                            <div class="panel info-box panel-white">
                                <div class="panel-body">
                                    <div class="info-box-stats">
                                        <p><span class="counter"><%= service_count %></span></p>
                                        <span class="info-box-title">Services offered</span>
                                    </div>
                                    <div class="info-box-icon">
                                        <i class="fa fa-cubes"></i>
                                    </div>
                                    <div class="info-box-progress">
                                        <div class="progress progress-xs progress-squared bs-n">
                                            <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="<%= service_count %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= service_count %>%">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6">
                            <div class="panel info-box panel-white">
                                <div class="panel-body">
                                    <div class="info-box-stats">
                                        <p class="counter"><%= sale_count %></p>
                                        <span class="info-box-title">Total sales</span>
                                    </div>
                                    <div class="info-box-icon">
                                        <i class="fa fa-cart-plus"></i>
                                    </div>
                                    <div class="info-box-progress">
                                        <div class="progress progress-xs progress-squared bs-n">
                                            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="<%= sale_count %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= sale_count %>%">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div><!-- Row -->
                
                </div><!-- Main Wrapper -->
                <% include ./partials/footer %>
            </div><!-- Page Inner -->
        </main><!-- Page Content -->
        <% include ./partials/menu-boxed %>
	

        <!-- Javascripts -->
        <script src="public/js/jquery-2.1.4.min.js"></script>
        <script src="public/js/jquery-ui.min.js"></script>
        <script src="public/js/pace.min.js"></script>
        <script src="public/js/jquery.blockui.js"></script>
        <script src="public/js/bootstrap.min.js"></script>
        <script src="public/js/jquery.slimscroll.min.js"></script>
        <script src="public/js/switchery.min.js"></script>
        <script src="public/js/jquery.uniform.min.js"></script>
        <script src="public/js/offcanvasmenueffects-classie.js"></script>
        <script src="public/js/offcanvasmenueffects-main.js"></script>
        <script src="public/js/waves.min.js"></script>
        <script src="public/js/3d-bold-navigation-main.js"></script>
        <script src="public/js/jquery.waypoints.min.js"></script>
        <script src="public/js/jquery.counterup.min.js"></script>
        <script src="public/js/toastr.min.js"></script>
        <script src="public/js/jquery.flot.min.js"></script>
        <script src="public/js/jquery.flot.time.min.js"></script>
        <script src="public/js/jquery.flot.symbol.min.js"></script>
        <script src="public/js/jquery.flot.resize.min.js"></script>
        <script src="public/js/jquery.flot.tooltip.min.js"></script>
        <script src="public/js/curvedLines.js"></script>
        <script src="public/js/MetroJs.min.js"></script>
        <script src="public/js/modern.min.js"></script>
        <script src="public/js/dashboard.js"></script>
        
    </body>
</html>